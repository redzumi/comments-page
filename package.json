{
  "name": "redzumi-react-kit",
  "version": "1.0.0",
  "scripts": {
    "webpack": "webpack",
    "eslint": "eslint",
    "babel": "babel",
    "rimraf": "rimraf",
    "prebuild:backend": "eslint --ignore-pattern '/src/service/web/*' src/",
    "build:backend": "babel --ignore service/web --copy-files src -d build",
    "prebuild:webservice": "mkdirp build/service/web && eslint src/service/web/service.js",
    "build:webservice": "babel src/service/web/service.js -o build/service/web/service.js",
    "prebuild:web": "rimraf build/service/web/assets",
    "build:web": "webpack",
    "start:web": "cd build/service/web && node service.js",
    "watch:web": "webpack --watch"
  },
  "author": "Redzumi",
  "license": "MIT",
  "dependencies": {
    "babel-cli": "^6.22.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-class-properties": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "compression-webpack-plugin": "^0.3.2",
    "css-loader": "^0.26.1",
    "eslint": "^3.14.1",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-async-await": "^0.0.0",
    "eslint-plugin-react": "^6.9.0",
    "extract-text-webpack-plugin": "beta",
    "html-webpack-plugin": "^2.28.0",
    "jquery": "^3.1.1",
    "koa": "next",
    "koa-router": "next",
    "koa-send": "^3.3.0",
    "mkdirp": "^0.5.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router": "^3.0.2",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "webpack": "^2.2.1",
    "webpack-merge": "^2.6.1"
  }
}
